<div class="container">
  <div class="row">
    <% @events.each do |event| %>
      <div class="card">
        <%= cl_image_tag event.photo, height: 300, crop: :fill %>
        <p class="card-text">
        <%= link_to event.name, event_path(event) %>
        <%= event.venue_name %>
        </p>
      </div>
    <% end %>
  </div>
</div>
